{
  "manifest_version": 2,
  "name": "Handy",
  "version": "0.0.1",
  "description": "Coming soon...",
  "content_scripts": [{
    "matches": ["<all_urls>"],
    "js": ["jquery-3.2.1.min.js", "app.js"],
    "css": ["app.css"]
  }],
  "web_accessible_resources": [
    "images/wikipedia.png",
    "images/twitter.png",
    "images/google.png",
    "images/googleMaps.png",
    "images/youtube.png",
    "images/spotify.png",
    "images/amazon.png",
    "images/ebay.png",
    "images/linkedin.png",
    "images/netflix.png",
    "images/stackoverflow.png",
    "images/pinterest.png",
    "images/rt.png",
    "images/imdb.png",
    "images/twitter.png"
  ],
  "permissions": [
    "contextMenus"
  ],
  "background": {
    "scripts": ["context.js"]
  },
  "browser_action": {
    "default_title": "Click Me"
  },
  "icons": {
    "16": "images/wikipedia.png"
  }
}